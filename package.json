{
  "name": "express-typescript",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch \"src/**/*.ts\" --exec \"ts-node\" src/index.ts",
    "build": "yarn && tsc",
    "start": "node ./dist/index.js",
    "format": "npx prettier --write .",
    "start-localhost": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.14",
    "husky": "^8.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "flutterwave-node-v3": "^1.1.6",
    "module-alias": "^2.2.3",
    "web3bridgemodels": "^0.0.1"
  },
  "engines": {
    "node": ">=14.20.1"
  }
}
